package main

import "github.com/smartcontractkit/cre-sdk-go/generator/protos"

func main() {
	if err := protos.Generate(&protos.CapabilityConfig{
		Category:     "{{.Category}}",
		Pkg:          "{{.Pkg}}",
		MajorVersion: {{.MajorVersion}},
		{{- if .PreReleaseTag }}
		PreReleaseTag: "{{.PreReleaseTag}}",
		{{- end }}
		Files: []string{
			{{- range .Files }}
			"{{.}}",
			{{- end }}
		},
	}); err != nil {
		panic(err)
	}
}
